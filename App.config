<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <appSettings>
    <!-- Twitter Streaming API settings.  Replace Twitter OAuth settings with your own -->
    <add key="oauth_consumer_key" value="**" />
    <add key="oauth_consumer_secret" value="**" />
    <add key="oauth_token" value="**" />
    <add key="oauth_token_secret" value="**" />

    <!-- Enter keywords, hashtags, or handles here.  Use comma to separate multiple entries -->
    <add key="twitter_keywords" value="Azure,Skype,XBox,Microsoft,Seattle" />

    <!-- Service Bus specific app setings for messaging connections -->
    <add key="EventHubConnectionString" value="**" />
    <add key="EventHubName" value="socialtwitter-eh"/>
  </appSettings>
</configuration>
